				function reply(type, id, origin) {
					$(origin).addClass('d-none');
					var limit = $('#limit-'+type+'-'+id).val();
					if($.isEmptyObject(limit)) {
						limit = '0';
					}
					$.ajax({
						url: "https://www.yourepo.com/ajax/"+type+"/"+id+"/" + limit,
						xhrFields: {withCredentials: true},
						crossDomain:true,
						success: function(data) {
							if(!$.isEmptyObject(data.replies)) {
								$('#list-replies-'+type+'-'+id).removeClass("d-none");
								$.each((data.replies), function( key, val) {
									$('#list-replies-'+type+'-'+id).append(val.html);
									embedStatus($('#list-replies-'+type+'-'+id).children().last());
								});
								$("time.timeago").timeago();
							}
							$("#max-"+type+"-"+id).val(data.limitMax);
							$("#limit-"+type+"-"+id).val(data.limit);
							if(data.limitMax < data.limit)
								$(origin).removeClass('d-none');
							$("#load-"+type+"-"+id).addClass('d-none');
							loaded = true;
							scrolled = false;
						},
						error:function(data){
							$(origin).removeClass('d-none');
							$("#load-"+id).addClass('d-none');
							loaded = true;
							scrolled = false;
						}
					});
				}
