				function copyToClipboard(text, element = document.body, button = false) {
					if(!button.attr('copying')) {
						var el = document.createElement('textarea');
						el.value = text;
						element.appendChild(el);
						el.select();
						var copied = document.execCommand('copy');
						element.removeChild(el);
						if(!copied) {
							window.prompt("Copy to clipboard:", text);
						} else {
							var prevText = button.html();
							button.attr('copying', 'true');
							button.html('<i class="far fa-fw fa-clipboard"></i>&nbsp;Copied');
							setTimeout(function() {button.html(prevText);button.removeAttr('copying');}, 2000);
						}
					}
				}
